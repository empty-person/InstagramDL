{
    "name": "Instagram Enhancer ",
    "version": "0.1",
    "description": "Enhances the Instagram experience and adds useful features.",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      ["js/background.js", "js/contentScript.js"]
    }],
    "permissions": [
        "tabs"
      
    ],
    "background": {
      "scripts": ["js/asfasf.js"],
      "persistent": false
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }